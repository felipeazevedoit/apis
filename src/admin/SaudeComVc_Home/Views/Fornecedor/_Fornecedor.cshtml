
@{
    ViewBag.Title = "_Fornecedor";
}

@model IEnumerable<SaudeComVoce.Models.ServicoViewModel>
<div class="modal fade bs-example-modal-lg" id="modalFornecedor" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel">
    <div class="modal-dialog modal-lg" role="document">
            <div class="form col-md-12" style="margin-top:0">
                <form name="form1" id="form1">
                    <div class="col-md-12" style="padding-bottom:3%;margin-top:3%">
                        <div class="row">
                            <div class="col-md-11">
                                <h1>Fornecedores da Saúde&nbsp;<i class="fas fa-first-aid"></i></h1>
                            </div>
                            <div class="col-md-1">
                                <button type="button" class="close" data-dismiss="modal">&times;</button>
                            </div>
                        </div>
                    </div>

                    <div class="col-md-12">

                        <div class="form-group container-fluid">
                            <label for="exampleInputEmail1" class="lbl">Razão Social</label>
                            <input type="text" class="form-control txt" id="nome" aria-describedby="emailHelp" style="padding-left:1%">
                            <span class="off" id="lblNome">*Obrigatório</span>
                        </div>

                        <div class="form-group col-md-6  container-fluid" style="margin-bottom:0">
                            <label for="exampleInputEmail1" class="lblc">CNPJ</label>
                            <input type="text" class="form-control txt" id="cnpj" aria-describedby="emailHelp" onKeyPress="MascaraCNPJ(form1.cnpj);" maxlength="18" onBlur="ValidarCNPJ(form1.cnpj);" style="padding-left:1%">
                            <span class="off" id="lblCnpj">*Obrigatório</span>
                        </div>

                        <div class="form-group col-md-6 container-fluid" style="margin-bottom:0">
                            <label>Área de Atuação</label>
                            <input type="text" class="form-control txt" id="atuacao" aria-describedby="emailHelp" style="padding-left:1%">
                            <span class="off" id="lblTelefone">*Obrigatório</span><br>
                        </div>

                        <div class="form-group col-md-12 container-fluid" style="margin-bottom:0">
                            <label>Nome do Responsável</label>
                            <input type="text" class="form-control txt" id="nomeResp" aria-describedby="emailHelp" style="padding-left:1%">
                            <span class="off" id="lblnomeResp">*Obrigatório</span><br>
                        </div>

                        

                        <div class="form-group col-md-6 container-fluid" style="margin-bottom:0">
                            <label>CPF do Responsável</label>
                            <input type="text" class="form-control txt" id="cpf" aria-describedby="emailHelp" style="padding-left:1%" onBlur="ValidarCPF(form1.cpf);" onKeyPress="MascaraCPF(form1.cpf);" maxlength="14">
                            <span class="off" id="lblCpf">*Obrigatório</span><br>
                        </div>

                        <div class="form-group col-md-6 container-fluid" style="margin-bottom:0">
                            <label>Email do Responsável</label>
                            <input type="text" class="form-control txt" id="emailResp" aria-describedby="emailHelp" style="padding-left:1%">
                            <span class="off" id="lblEmailResp">*Obrigatório</span><br>
                        </div>

                    </div>

                    <div class="col-md-12 ">
                        <div class="form-group col-md-6 container-fluid">
                            <label>Contato Comercial</label>
                            <input type="text" style="padding-left:1%" class="form-control" placeholder="" id="telefone" name="telefone" onKeyPress="TelMask();" maxlength="14">
                            <span class="off" id="lblTelefone">*Obrigatório</span>
                        </div>
                        <div class="form-group col-md-6 container-fluid" style="margin-bottom:0">
                            <label>Site</label>
                            <input type="text" class="form-control txt" id="site" aria-describedby="emailHelp" style="padding-left:1%">
                            <span class="off" id="lblSite">*Obrigatório</span><br>
                        </div>
                    </div>

                    <div class="col-md-12">

                        <div class="form-group col-md-3 container-fluid">
                            <label for="exampleInputEmail1" class="lblc">CEP</label>
                            <input type="text" class="form-control txt" id="cep" aria-describedby="emailHelp" onfocusout="preencherEndereco($('#cep').val())" onKeyPress="MascaraCep(form1.cep);" maxlength="9" style="padding-left:1%">
                            <span class="off" id="lblCep">*Obrigatório</span>
                        </div>

                        <div class="form-group col-md-5 container-fluid">
                            <label for="exampleInputEmail1" class="lblc">Endereço</label>
                            <input type="text" class="form-control txt" id="local" aria-describedby="emailHelp" style="padding-left:1%">
                            <span class="off" id="lblLocal">*Obrigatório</span>
                        </div>

                        <div class="form-group col-md-2 container-fluid">
                            <label for="exampleInputEmail1" class="lblc">Complemento</label>
                            <input type="text" class="form-control txt" id="complemento" aria-describedby="emailHelp" onfocusout="preencherEndereco($('#cep').val())">
                            <span class="off" id="lblComplemento">*Obrigatório</span>
                        </div>

                        <div class="form-group col-md-2 container-fluid">
                            <label for="exampleInputEmail1" class="lblc">Número</label>
                            <input type="number" class="form-control txt" id="numero" aria-describedby="emailHelp" style="padding-left:1%">
                            <span class="off" id="lblNumero">*Obrigatório</span>
                        </div>
                    </div>


                    <div class="col-md-12">
                        <div class="form-group col-md-6 container-fluid">
                            <label for="exampleInputEmail1" class="lblc">Email</label>
                            <input type="email" class="form-control txt" id="email" aria-describedby="emailHelp" onfocusout="validarEmail()" style="padding-left:1%">
                            <span class="off" id="lblEmail">*Obrigatório</span>
                        </div>

                        <div class="form-group col-md-6 container-fluid">
                            <label for="exampleInputEmail1" class="lblc">Confirmar Email</label>
                            <input type="email" class="form-control txt" id="confirmarEmail" onfocusout="validarCEmail()" aria-describedby="emailHelp" style="padding-left:1%">
                            <span class="off" id="lblConfirmarEmail">*Obrigatório</span>
                        </div>
                    </div>
                    <div class="col-md-12">
                        <div class="col-md-7"></div>

                        <div class="form-group col-md-6 container-fluid">
                            <label for="exampleInputEmail1" class="lblc">Senha</label>
                            <input type="password" class="form-control txt" id="senha"
                                   aria-describedby="emailHelp" style="padding-left:1%">
                            <span class="off" id="lblSenha">*Obrigatório</span>
                        </div>

                        <div class="form-group col-md-6 container-fluid">
                            <label for="exampleInputEmail1" class="lblc"> Confirmar senha</label>
                            <input type="password" class="form-control txt" id="confirmarSenha"
                                   aria-describedby="emailHelp" style="padding-left:1%">
                            <span class="off" id="lblConfirmarSenha">*Obrigatório</span>
                        </div>
                        <span style="color:green" id="lblSpan" class="pull-right fade">Cadastro realizado com sucesso</span>
                        <span id="lblErro" class="pull-left off"></span>
                        <div class="row">
                            <div class="col-md-12">
                                <div>
                                    <a href="#" class="link pull-left" onclick="carregaModalLista()">Voltar</a>
                                    <button type="button" id="btnCadastrar" class="btn btn-default btn-lg btn-cadastrar pull-right" onclick="cadastrarFornecedor()">Cadastrar</button>

                                </div>
                            </div>
                        </div>
                    </div>
                </form>

            </div>
        </div>
        </div>
